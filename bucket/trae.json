{
    "version": "1.0.12801",
    "description": "An AI-powered IDE that makes programming more enjoyable and efficient.",
    "homepage": "https://www.trae.ai",
    "license": {
        "identifier": "Unknown",
        "url": "https://www.trae.ai/terms-of-service"
    },
    "notes": [
        "Add Trae as a context menu option by running 'reg import \"$dir\\install-context.reg\"'",
        "For file associations, run 'reg import \"$dir\\install-associations.reg\"'"
    ],
    "architecture": {
        "64bit": {
            "url": "https://lf-cdn.trae.ai/obj/trae-ai-sg/pkg/app/releases/stable/1.0.12801/win32/Trae-Setup-x64.exe",
            "hash": "3f00cccd86e2e753d2723228ffb6b4fc63a759f7e7f10183fc45b65178075099",
            "extract_dir": "{code_GetDestDir}"
        }
    },
    "innosetup": true,
    "env_add_path": "bin",
    "bin": [["bin/trae.cmd", "trae"]],
    "shortcuts": [
        [
            "Trae.exe",
            "Trae",
            "--user-data-dir=\"$dir\\data\\user-data\" --extensions-dir=\"$dir\\data\\extensions\" --agent-extensions-dir=\"$dir\\data\\agent-extensions\""
        ]
    ],
    "persist": "data",
    "post_install": [
        "$dirpath = \"$dir\".Replace('\\', '\\\\')",
        "$exepath = \"$dir\\Trae.exe\".Replace('\\', '\\\\')",
        "'install-associations', 'uninstall-associations', 'install-context', 'uninstall-context' | ForEach-Object {",
        "  if (Test-Path \"$bucketsdir\\extras\\scripts\\vscode\\$_.reg\") {",
        "    $content = Get-Content \"$bucketsdir\\extras\\scripts\\vscode\\$_.reg\"",
        "    $content = $content.Replace('$codedir', $dirpath)",
        "    $content = $content.Replace('$code', $exepath)",
        "    $content = $content.Replace('&Code', '&Trae')",
        "    if ($global) {",
        "      $content = $content.Replace('HKEY_CURRENT_USER', 'HKEY_LOCAL_MACHINE')",
        "    }",
        "    $content | Set-Content -Path \"$dir\\$_.reg\"",
        "  }",
        "}",
        "if (!(Test-Path \"$dir\\data\\extensions\") -and (Test-Path \"$env:USERPROFILE\\.trae\\extensions\")) {",
        "  info '[Portable Mode] Copying extensions...'",
        "  Copy-Item \"$env:USERPROFILE\\.trae\\extensions\" \"$dir\\data\" -Recurse",
        "}",
        "if (!(Test-Path \"$dir\\data\\user-data\") -and (Test-Path \"$env:AppData\\Trae\")) {",
        "  info '[Portable Mode] Copying user data...'",
        "  Copy-Item \"$env:AppData\\Trae\" \"$dir\\data\\user-data\" -Recurse",
        "}",
        "$extensions_file = \"$dir\\data\\extensions\\extensions.json\"",
        "if ((Test-Path \"$extensions_file\")) {",
        "  info 'Adjusting path in extensions file...'",
        "    (Get-Content \"$extensions_file\") -replace '(?<=trae(/|\\\\\\\\)).*?(?=(/|\\\\\\\\)data(/|\\\\\\\\)extensions)', $version | Set-Content \"$extensions_file\"",
        "}",
        ""
    ],
    "uninstaller": {
        "script": [
            "if ($cmd -eq 'uninstall')",
            "{",
            "   reg import \"$dir\\uninstall-context.reg\" ",
            "   reg import \"$dir\\uninstall-associations.reg\" ",
            "}"
        ]
    },
    "checkver": {
        "url": "https://api.trae.ai/icube/api/v1/native/version/trae/latest",
        "jsonpath": "$.data.manifest.win32.version"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://lf-cdn.trae.ai/obj/trae-ai-sg/pkg/app/releases/stable/$version/win32/Trae-Setup-x64.exe"
            }
        }
    }
}
